{
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "extends": ["airbnb-base", "plugin:prettier/recommended", "plugin:jest/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "import/extensions": "off",
        "no-continue": "warn",
        "no-restricted-syntax": "warn"
    }
}
